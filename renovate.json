{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableRateLimiting"
  ],
  "timezone": "Asia/Tokyo",
  "schedule": [
    "every weekend"
  ],
  "packageRules": [
    {
      "groupName": "prettier",
      "matchPackageNames": [
        "prettier",
        "prettier{/,}**"
      ]
    }
  ]
}
